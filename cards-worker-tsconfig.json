{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "./dist-cards-worker",
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "classic",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/cards-worker"
  ]
}